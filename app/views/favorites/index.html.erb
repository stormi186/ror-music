<div class="row">
  <div class="col-md-7">
    <h1> Favorite tracks </h1>
    <hr>
<%= will_paginate @favorites, renderer: BootstrapPagination::Rails %>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Duration</th>
          <th>Genre</th>
          <th>Artist</th>
          <th>Album</th>
          <th>Year</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <%= render @favorites %>
      </tbody>
    </table>
  </div>