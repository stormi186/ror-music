<tr>
  <td><%= link_to track.name, superadmin_track_path(track) %></td>
  <td><%= track.duration %></td>
  <td><%= track.genre.name %></td>
  <td><%= track.artist.name %></td>
  <td><%= track.album.name %></td>
  <td>
  <div class="btn-group">
    <%= link_to 'Edit', edit_superadmin_track_path(track), class: 'btn btn-sm btn-primary' %>
    <%= link_to 'Delete', superadmin_track_path(track), method: :delete, data: {confirm: "Are you sure?"}, class: 'btn btn-sm btn-danger' %>
    </div>
  </td>
</tr>
